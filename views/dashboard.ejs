<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/styles.css">
  <title>Checklist Dashboard</title>
</head>
<body>
  <h1>Checklist Evaluation</h1>
  <table>
    <thead>
      <tr>
        <th>Rule</th>
        <th>Status</th>
      </tr>
    </thead>
    <tbody>
      <% checklistResults.forEach(result => { %>
        <tr>
          <td><%= result.name %></td>
          <td class="<%= result.status === 'Passed' ? 'passed' : 'failed' %>"><%= result.status %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
